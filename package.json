    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "copyfiles": "^2.4.1",
    "create-react-app": "^5.0.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "prettier": "^2.3.1",
    "react-scripts": "4.0.3",
    "rollup": "^2.72.0",
